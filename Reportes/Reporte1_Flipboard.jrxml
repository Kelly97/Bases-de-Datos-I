	<queryString language="SQL">
		<![CDATA[select to_char(a.fecha_de_creacion, 'FMMONTH') MES, COUNT(to_char(a.fecha_de_creacion, 'FMMONTH')) CREADAS_X_MES, SUM(B.SEGUIDORES) SEGUIDORES
from tbl_revistas a
LEFT JOIN (
    SELECT A.CODIGO_REVISTA, COUNT(1) SEGUIDORES
    FROM TBL_REVISTAS_SEGUIDAS A
    GROUP BY A.CODIGO_REVISTA
    ) B
ON A.CODIGO_REVISTA = B.CODIGO_REVISTA
group by to_char(a.fecha_de_creacion, 'FMMONTH')
ORDER BY SEGUIDORES DESC, CREADAS_X_MES DESC]]>
	</queryString>
